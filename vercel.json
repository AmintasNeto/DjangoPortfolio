{
    "builds": [
      {
        "src": "DjangoPortifolio/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": { "maxLambdaSize": "15mb" , "runstime": "python3.9"}
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "DjangoPortifolio/wsgi.py"
      }
    ]
}